        <item>"mobile_cbs,12,0,2,300000,true"    </item>
        <item>"mobile_dm,34,0,3,300000,true"</item>
        <item>"mobile_wap,35,0,3,300000,true"</item>
        <item>"mobile_net,36,0,3,300000,true"</item>
        <item>"mobile_cmmail,37,0,3,300000,true"</item>
